@model Insights.Models.DateClassModel
@{
    ViewBag.Title = "MonthlyReport";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<style>
    .hidden {
        display: none;
    }
    .refreshicon{
        font-size: 0.7em;
        cursor:pointer;
    }
    .dt-buttons a{
            width:32px;
           line-height: 0;
           margin-top: 20px;
        }
        .dt-buttons span{
            font-size:14px;
            margin-left:-13px;
        }
        .ti-calendar{
            background-color:white;
        }
        .dateProp{
            background-color:white !important;
        }
</style>

